	<!-- 相对布局 -->
	<!-- 按钮 -->
	<control name = "按钮">
		<attr name = "名称" default="按钮1"></attr>
		<attr name = "宽度" default="匹配内容"></attr>
		<attr name = "高度" default="匹配内容"></attr>
		<attr name = "内容对齐"></attr>
		<attr name = "顶部填充"></attr>		
		<attr name = "底部填充"></attr>
		<attr name = "左边填充"></attr>		
		<attr name = "右边填充"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>		
		<attr name = "文本" default="我是按钮1"></attr>
		<attr name = "文本大小"></attr>
			
		<attr name = "文本样式"></attr>
		<attr name = "文本颜色"></attr>
        <attr name = "权重"></attr>
        <attr name = "按下效果"></attr>	
        <attr name = "弹起效果"></attr>	
        <attr name = "是否隐藏"></attr>

		<attr name = "点击事件" default="自定义功能名(必须在代码中定义)"></attr>
		<attr name = "长按时间"></attr>
	</control>
	
	<!-- 标签 -->
	<control name = "标签">
		<attr name = "名称" default="标签1"></attr>
		<attr name = "宽度" default="匹配内容"></attr>
		<attr name = "高度" default="匹配内容"></attr>
		<attr name = "内容对齐"></attr>
		<attr name = "顶部填充"></attr>		
		<attr name = "底部填充"></attr>
		<attr name = "左边填充"></attr>		
		<attr name = "右边填充"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>		
		<attr name = "权重"></attr>

		<attr name = "文本" default="标签1"></attr>
		<attr name = "文本大小"></attr>
		<attr name = "文本样式"></attr>
		<attr name = "文本颜色"></attr>
		<attr name = "文本链接方式"></attr>
		<attr name = "文本链接地址"></attr>
        <attr name = "是否隐藏"></attr>

		<attr name = "点击事件"></attr>
		<attr name = "长按事件"></attr>
	</control>
	
	<!-- 编辑框 -->
	<control name = "编辑框">
		<attr name = "名称" default="编辑框1"></attr>
		<attr name = "宽度" default="匹配内容"></attr>
		<attr name = "高度" default="匹配内容"></attr>
		<attr name = "内容对齐"></attr>
		<attr name = "顶部填充"></attr>		
