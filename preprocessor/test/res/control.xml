		<attr name = "底部填充"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>
        <attr name = "权重"></attr>
        <attr name = "是否隐藏"></attr>
        <attr name = "状态改变事件"></attr>
	</control>

    <control name = "配置">
        <attr name = "启动方式"></attr>
    </control>
	
	<!-- 浏览器 -->
	<control name = "浏览器">
		<attr name = "名称" default="浏览器1"></attr>
		<attr name = "宽度" default="填充"></attr>
		<attr name = "高度" default="填充"></attr>
		<attr name = "地址" default="bbs.tyuyan.com"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>
        <attr name = "跳转方式" default="内部"></attr>
        <attr name = "权重"></attr>
        <attr name = "是否隐藏"></attr>

        <attr name = "加载开始事件"></attr>
		<attr name = "加载完成事件"></attr>
        <attr name = "加载出错事件"></attr>
                
	</control>
	
	<!-- 拥有默认值的属性 -->
	<attrvalues>
		<attr name="宽度">
			<value>填充</value>	
			<value>匹配内容</value>	
		</attr>	
		<attr name="高度">
			<value>填充</value>	
			<value>匹配内容</value>	
		</attr>	
		<attr name="排列方向">
			<value>纵向排列</value>	
			<value>横向排列</value>	
		</attr>	
		<attr name="控件对齐">
			<value>顶部</value>	
			<value>底部</value>	
			<value>左对齐</value>	
			<value>右对齐</value>	
			<value>居中</value>	
			<value>纵向居中</value>	
			<value>横向居中</value>	
			<value>填充</value>	
			<value>纵向填充</value>	
			<value>横向填充</value>	
		</attr>	
		
		<attr name="文本样式">
			<value>正常</value>	
			<value>粗体</value>	
		</attr>	
		<attr name="默认选中">
			<value>是</value>	
			<value>否</value>	
		</attr>	
		
		<attr name="文本链接方式">
			<value>默认</value>
			<value>电话</value>
			<value>邮件</value>
			<value>网络</value>
			<value>短信</value>
		</attr>
		<attr name="文本框类型">
			<value>默认</value>	
			<value>数字框</value>	
			<value>密码框</value>	
		</attr>	
		<attr name="图文混排">
			<value>开启</value>	
			<value>关闭</value>	
		</attr>	
        <attr name="内容对齐">
            <value>顶部</value>	
			<value>底部</value>	
			<value>左对齐</value>	
			<value>右对齐</value>	
			<value>居中</value>	
			<value>纵向居中</value>	
			<value>横向居中</value>	
			<value>填充</value>	
			<value>纵向填充</value>	
			<value>横向填充</value>	
        </attr>
        <attr name="跳转方式">
			<value>外部</value>	
			<value>内部</value>	
		</attr>	
        <attr name="启动方式">
			<value>默认</value>	
			<value>无悬浮窗</value>	
            <value>悬浮窗</value>	
            <value>快速启动</value>  
		</attr>	
               
        <attr name="是否隐藏">
			<value>是</value>	
			<value>否</value>	
		</attr>

	</attrvalues>
</controls>

