		<attr name = "左边填充"></attr>
		<attr name = "右边填充"></attr>
		<attr name = "顶部填充"></attr>
		<attr name = "底部填充"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>  
        <attr name = "权重"></attr>
        <attr name = "是否隐藏"></attr>

        <attr name = "状态改变事件"></attr>
        <attr name = "点击事件"></attr>
	</control>
	
	<!-- 复选框 -->
	<control name = "复选框">
		<attr name = "名称" default="复选框1"></attr>
		<attr name = "宽度" default="匹配内容"></attr>
		<attr name = "高度" default="匹配内容"></attr>
		<attr name = "文本" default="复选框1"></attr>
		<attr name = "文本大小"></attr>
		<attr name = "文本颜色"></attr>
		<attr name = "文本样式"></attr>
