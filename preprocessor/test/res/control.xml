<?xml version="1.0" encoding="utf-8"?>
<controls>
	<!-- 线性布局 -->
	<control name = "线性布局">
		<attr name = "名称" default = "线性布局1" ></attr>
		<attr name = "宽度" default = "填充" ></attr>
		<attr name = "高度" default = "填充" ></attr>
		<attr name = "排列方向" default = "横向排列" ></attr>
		<attr name = "顶部填充"></attr>		
