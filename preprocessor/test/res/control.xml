		<attr name = "右边填充"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>
        <attr name = "权重"></attr>

		<attr name = "文本" default="编辑框1"></attr>
		<attr name = "文本大小"></attr>
		<attr name = "文本样式"></attr>
		<attr name = "文本颜色"></attr>		
		<attr name = "字体个数"></attr>
		<attr name = "文本框类型" default="默认"></attr>
		<attr name = "多行" default="否"></attr>
		<attr name = "图文混排"></attr>
		<attr name = "提示语"></attr>
        <attr name = "是否隐藏"></attr>
        <attr name = "加载背景"></attr>
