		<attr name = "失去焦点事件"></attr>
		<attr name = "键盘事件"></attr>
		<attr name = "输入变化事件"></attr>
	</control>
	
	<!-- 图片框 -->
	<control name = "图片框">
		<attr name = "名称" default="图片框1"></attr>
		<attr name = "宽度" default="匹配内容"></attr>
		<attr name = "高度" default="匹配内容"></attr>
		<attr name = "内容对齐"></attr>
		<attr name = "顶部填充"></attr>		
		<attr name = "底部填充"></attr>
		<attr name = "左边填充"></attr>		
		<attr name = "右边填充"></attr>
		<attr name = "控件对齐"></attr>
		<attr name = "左边距离"></attr>
		<attr name = "右边距离"></attr>
		<attr name = "顶部距离"></attr>
		<attr name = "底部距离"></attr>
        <attr name = "权重"></attr>
