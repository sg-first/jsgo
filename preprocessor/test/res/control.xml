        <attr name = "是否隐藏"></attr>

		<attr name = "点击事件"></attr>
		<attr name = "长按事件"></attr>
	</control>
	
	<!-- 单选框 -->
	<control name = "单选框">
		<attr name = "名称" default="单选框1"></attr>
		<attr name = "宽度" default="匹配内容"></attr>
		<attr name = "高度" default="匹配内容"></attr>
		<attr name = "文本" default="单选框1"></attr>
		<attr name = "文本大小"></attr>
		<attr name = "文本颜色"></attr>
